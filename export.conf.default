[export]

# Logfile
logfile = export.log

# Print debug output
debug = False

# Relative path to directory containing timestamp-prefixed JSON export files
export_dir = data/export

# Relative path to directory containing nodes aggregates JSON files
export_aggr_dir = data/export_aggr

# How many connections should be returned per location in aggregates export
export_aggr_peers_max_count = 10